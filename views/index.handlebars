<div class="row">

        {{#each news}}
            <div class="col-md-3">

        <div class="card" style="width: 18rem;">
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{this.headline}}</h5>
                <p class="card-text">{{this.summary}}</p>
                <a href='{{this.URL}}'>URL</a>
            </div>
        </div>
    </div>
        {{/each}}

</div>